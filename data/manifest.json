{
    "manifest_version": 2,
    "name": "SelectionEnabler",
    "short_name": "SelectionEnabler",
    "description": "Disable text selection block.",
    "version": "1",
    "browser_action": {
        "default_icon": "iconon.png",
        "default_title": "Let's select some text!"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["custom.js"]
      }
    ],
  "permissions": [
    "identity",
    "tabs",
    "activeTab",
    "*://*/*"
  ]
}
